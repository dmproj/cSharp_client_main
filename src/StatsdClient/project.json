{
  "version": "1.0.0-*",
  "authors": [ "Goncalo Pereira", "Darrell Mozingo" ],
  "title":  "Statsd C# Client",
  "description": "Statsd client for C#, providing a full set of counter/timer/gauge/set functionality in an easy to use static. Helpers are provided for easily timing chunks of code too.",
  "name":  "StatsdClient",
  "packOptions": {
    "releaseNotes": "See CHANGELOG.md in the project's GitHub repository.",
    "tags": [ "stats", "statsd", "metrics" ],
    "licenseUrl": "https://github.com/Pereingo/statsd-csharp-client/blob/master/MIT-LICENSE.md",
    "projectUrl": "https://github.com/Pereingo/statsd-csharp-client",
    "repository": {
      "type:": "git",
      "url": "https://github.com/Pereingo/statsd-csharp-client"
    }
  },

  "buildOptions": {
    "warningsAsErrors": true
  },

  "frameworks": {
    "net45": {
      "buildOptions": {
        "define": [ "NETFULL" ]
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.Net.NameResolution": "4.0.0"
      },
      "imports": "dnxcore50"
    }
  }
}
